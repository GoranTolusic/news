{
  "name": "expresstype",
  "version": "1.0.0",
  "description": "Simple blog news API",
  "main": "index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx tsc",
    "start": "ts-node index.ts",
    "installAndRun": "rm -rf node_modules/ && rm -rf package-lock.json && npm i && npm run seed && npm run start",
    "seed": "ts-node ./scripts/seedNews.ts",
    "dockerize": " npm run dockerImage && npm run dockerRun ",
    "dockerImage": "docker build -t news .",
    "dockerRun": "docker run news"
  },
  "author": "Goran Tolusic",
  "license": "ISC",
  "dependencies": {
    "@tsed/exceptions": "^7.13.2",
    "@types/body-parser": "^1.19.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.9.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongodb": "^4.13.0",
    "nodemailer": "^6.9.0",
    "reflect-metadata": "^0.1.13",
    "ts-newsapi": "^1.0.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.11",
    "uid": "^2.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "concurrently": "^7.6.0",
    "nodemon": "^2.0.20",
    "ts-node": "10.7.0",
    "typescript": "^4.9.4"
  }
}
